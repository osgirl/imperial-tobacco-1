  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "build": "rimraf dist && yarn run migrate && webpack --config config/webpack.prod.js --bail",
    "client": "webpack-dev-server --config config/webpack.dev.js --inline",
    "server": "node server/app.js",
    "test": "mocha --recursive tests",
    "server:nodemon": "NODE_ENV=dev nodemon server/app.js",
    "migrate": "DEBUG=* node migrations/migrate.js",
    "server:demo-build": "NODE_ENV=testing DEGUG=* node migrations/migrate && NODE_ENV=testing yarn run build",
    "unit:demo-build": "NODE_ENV=testing mocha --recursive tests --reporter mocha-tap-reporter > test.tap"
  },
